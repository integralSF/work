ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda-11.7/bin/nvcc

cflags = -pthread -B /home/zjh/anaconda3/envs/pf/compiler_compat -Wl,--sysroot=/ -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -Wstrict-prototypes -fPIC -I/home/zjh/anaconda3/envs/pf/lib/python3.8/site-packages/torch/include -I/home/zjh/anaconda3/envs/pf/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -I/home/zjh/anaconda3/envs/pf/lib/python3.8/site-packages/torch/include/TH -I/home/zjh/anaconda3/envs/pf/lib/python3.8/site-packages/torch/include/THC -I/usr/local/cuda-11.7/include -I/home/zjh/anaconda3/envs/pf/include/python3.8 -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=chamfer_3D -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/home/zjh/anaconda3/envs/pf/lib/python3.8/site-packages/torch/include -I/home/zjh/anaconda3/envs/pf/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -I/home/zjh/anaconda3/envs/pf/lib/python3.8/site-packages/torch/include/TH -I/home/zjh/anaconda3/envs/pf/lib/python3.8/site-packages/torch/include/THC -I/usr/local/cuda-11.7/include -I/home/zjh/anaconda3/envs/pf/include/python3.8 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=chamfer_3D -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_75,code=compute_75 -gencode=arch=compute_75,code=sm_75 -std=c++14
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags





build /home/zjh/workspace/PFNet/tools/chamfer3D/build/temp.linux-x86_64-cpython-38/chamfer3D.o: cuda_compile /home/zjh/workspace/PFNet/tools/chamfer3D/chamfer3D.cu
